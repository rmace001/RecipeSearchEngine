         (function() {

  var seGlobalHome = "https://www.seriouseats.com";
    var seStaff = [{"name": "Ariel Kanter", "slug": "user/profile/ariel kanter"}, {"name": "Daniel Gritzer", "slug": "editors/daniel-gritzer"}, {"name": "Ed Levine", "slug": "editors/ed-levine"}, {"name": "Elazar Sontag", "slug": "editors/elazar-sontag"}, {"name": "J. Kenji L\u00f3pez-Alt", "slug": "editors/j-kenji-lopez-alt"}, {"name": "Kristina Bornholtz", "slug": "user/profile/kristina bornholtz"}, {"name": "Kristina Razon", "slug": "editors/kristina-razon"}, {"name": "Miranda Kaplan", "slug": "editors/miranda-kaplan"}, {"name": "Niki Achitoff-Gray", "slug": "editors/niki-achitoff-gray"}, {"name": "Paul Cline", "slug": "user/profile/paulcline"}, {"name": "Sasha Marx", "slug": "editors/sasha-marx"}, {"name": "Sho Spaeth", "slug": "editors/sho-spaeth"}, {"name": "Stella Parks", "slug": "editors/stella-parks"}, {"name": "Tim Aikens", "slug": "user/profile/aikenst"}, {"name": "Vicky Wasik", "slug": "user/profile/vickywasik"}, {"name": "maggielee", "slug": "user/profile/maggielee"}];


  window.seGlobalObjects = window.seGlobalObjects || {};

  function Comments() {
    this.outerWrapSelector = '.comments-container';
    this.userLinksSelector = '.user-link-wrap';
    this.loginToAddSelector = '[js-class="login-to-comment"]';
    this.addCommentSelector = '.post-comment-wrapper';
    this.ratingStarSelector = '[js-class="rating-star"]';
    this.$outerWrap = $(this.outerWrapSelector);
    this.$userLinks = $(this.userLinksSelector);
    this.$loginToAdd = $(this.loginToAddSelector);
    this.$addComment = $(this.addCommentSelector);
    this.$ratingStar = $(this.ratingStarSelector);

    this.commentSections = [];

    this.numCommentsPerPage = 15;

    this.init();
  }

  Comments.prototype = {
    init: function() {
      var obj = this;
      // obj.update();
      obj.addEventListeners();
    },

    update: function() {
      var obj = this;
      obj.$outerWrap = $(obj.outerWrapSelector);
      obj.$userLinks = $(obj.userLinksSelector);
      obj.$loginToAdd = $(obj.loginToAddSelector);
      obj.$addComment = $(obj.addCommentSelector);
      obj.$ratingStar = $(obj.ratingStarSelector);
      if (typeof _cameo !== "undefined" && typeof _cameo.checkLoginCookie === "function") {
        _cameo.checkLoginCookie(obj.loginSuccess.bind(obj), obj.loginFailure.bind(obj));
      }
    },

    addNewSection: function(postID, showComments, comments) {
      var obj = this;
      obj.commentSections.push(new CommentSection(postID, showComments, comments, obj.$outerWrap.last()));
    },

    updateUserLinks: function(loggedIn, username) {
      var obj = this;
      if (loggedIn) {
        obj.$userLinks.html('<span>Hello, ' + username + '!</span> | <a href="#" class="logout-link">Sign Out</a>');
      } else {
        obj.$userLinks.html("<a class=\"login-link\" href=\"" + seGlobalHome + "/login\">Sign In</a><span> or </span><a class=\"register-link\" href=\"" + seGlobalHome + "/register\">Register</a>");
      }
    },

    updateFormArea: function(loggedIn) {
      var obj = this;
      if (loggedIn) {
        obj.$loginToAdd.hide();
        obj.$addComment.show();
      } else {
        obj.$addComment.hide();
        obj.$loginToAdd.show();
      }
    },

    updateCommentsSections: function(loggedIn, username) {
      var obj = this;
      if (typeof username !== "string") {
        username = "Serious Eater";
      }
      obj.updateUserLinks(loggedIn, username);
      obj.updateFormArea(loggedIn);
    },

    loginSuccess: function(username) {
      this.updateCommentsSections(true, username);
    },

    loginFailure: function() {
      this.updateCommentsSections(false);
    },

    logoutSuccess: function() {
      this.updateCommentsSections(false);
    },

    // stars: function(star) {
    //   if (hovering) {
    //     var $stars = $(this).siblings("i").andSelf();
    //     var id = $(this).index();
    //     $stars.slice(0, id + 1).removeClass("icon-nav-star-empty").addClass("icon-nav-star-full");
    //   } else {
    //     var $stars = $(this).siblings("i").andSelf().not(".fixed");
    //     var id = $(this).index();
    //     $stars.slice(0, id + 1).removeClass("icon-nav-star-full").addClass("icon-nav-star-empty");
    //   }
    // },

    addEventListeners: function() {
      var obj = this,
          $document = $(document);
      $document.on("login-success", function(){
        var username = _cameo.getCookie("cameo_username");
        obj.loginSuccess(username);
      });
      $document.on("login-failure", obj.loginFailure.bind(obj));
      $document.on('click', '.logout-link', function(e){
        e.preventDefault();
        _cameo.logout(obj.logoutSuccess.bind(obj));
      });
      // $document.on({
      //   mouseenter: function () {
      //     obj.stars(this);
      //   },
      //   mouseleave: function () {
      //     obj.stars(this);
      //   }
      // }, obj.ratingStarSelector);
    }
  };

  function CommentSection(postID, showComments, comments, $wrapEle) {
    this.postID = postID;
    this.showComments = showComments;
    this.comments = comments;
    this.$section = $wrapEle;
    this.init();
  }

  CommentSection.prototype = {
    init: function() {
      var obj = this;
      obj.addEventListeners();
    },
    addEventListeners: function() {}
  };

  seGlobalObjects.commentsController = new Comments();


})();

var numCommentsPerPage = 15;
var _seStaff = [];

  _seStaff = [{"name": "Ariel Kanter", "slug": "user/profile/ariel kanter"}, {"name": "Daniel Gritzer", "slug": "editors/daniel-gritzer"}, {"name": "Ed Levine", "slug": "editors/ed-levine"}, {"name": "Elazar Sontag", "slug": "editors/elazar-sontag"}, {"name": "J. Kenji L\u00f3pez-Alt", "slug": "editors/j-kenji-lopez-alt"}, {"name": "Kristina Bornholtz", "slug": "user/profile/kristina bornholtz"}, {"name": "Kristina Razon", "slug": "editors/kristina-razon"}, {"name": "Miranda Kaplan", "slug": "editors/miranda-kaplan"}, {"name": "Niki Achitoff-Gray", "slug": "editors/niki-achitoff-gray"}, {"name": "Paul Cline", "slug": "user/profile/paulcline"}, {"name": "Sasha Marx", "slug": "editors/sasha-marx"}, {"name": "Sho Spaeth", "slug": "editors/sho-spaeth"}, {"name": "Stella Parks", "slug": "editors/stella-parks"}, {"name": "Tim Aikens", "slug": "user/profile/aikenst"}, {"name": "Vicky Wasik", "slug": "user/profile/vickywasik"}, {"name": "maggielee", "slug": "user/profile/maggielee"}];


  function comment_success(post_id, response)
  {
    $("div#post-comment-status-" + post_id).html(response["message"]);
    $("div#post-comment-status-" + post_id).removeClass("hidden");
    $("div#post-comment-" + post_id).addClass("hidden");
  } // comment_success

  function comment_failure(post_id, response)
  {
    $("div#post-comment-status-" + post_id).html(response["message"]);
    $("div#post-comment-status-" + post_id).removeClass("hidden");
  } // comment_failure

  $(document).on("keyup", "textarea#comment-text", function() 
  {
    comment = $(this).val();
    post_id = $(this).data("post-id");
    if(comment == "") {comment = "&nbsp;";}
    $("p#preview-" + post_id).html(comment.replace(/\n/g, "<br />").replace(/\n\n+/g, '<br /><br />').replace(/(&lt;\/?)script/g,"$1noscript"));
  });

  $(document).on("submit", "form#comments_form", function(e)
  {
    e.preventDefault();

    var post_id = $(this).find("input[name=post_id]").val();
    var comment_text = $(this).find("textarea").val();
    var comment_rating = $(this).find("input[name=star_rating]").val();

    document.getElementById("comment-post-" + post_id).disabled=true; 
    document.getElementById("comment-post-" + post_id).value = "Got it, thanks!";

    _cameo.newComment(post_id, comment_text, comment_success, comment_failure, comment_rating);
  });

function commentDisplayButtonClick($button) {
  
  if (!$button)
    return;

  var thishref = $button.attr('href');
  var post_id = $button.data("post-id");
  
  $button.hide();
  $('a.hide-comments[href="' + thishref + '"]').removeClass("hidden");
  $('a.hide-comments[href="' + thishref + '"]').css("display","");
  
  // showCommentSection(post_id);
  $(thishref).removeClass('hidden');

// #*
// #if $comments_open
//     if(!user || !user.is_authenticated) {
//       document.getElementById("post-comment-" + post_id).style.display = 'none';
//     }
//     setTimeout(add_message(post_id), 500);
// #end if
// *#

} // commentDisplayButtonClick

function handlePaginationClick(page_index, pagination_container) {

  var comment_count = this["comments"].length;
  var max_num = Math.min((page_index+1) * numCommentsPerPage, comment_count);

  var commentRange = '';
  if ((max_num < comment_count) || (max_num > numCommentsPerPage)) {
    commentRange += "(" + (page_index*numCommentsPerPage + 1) + " - " + max_num + ") of ";
  }

  $('#commentRange-' + this["post_id"]).html(commentRange);
  showComments(this["blog_id"], this["post_id"], this["comments"], page_index*numCommentsPerPage, max_num, this["author_username"]);
  
  if (jumpToTopOfComments) {
    document.getElementById('comments-' + this["post_id"]).scrollIntoView();
  }

  return false;
} // handlePaginationClick

$(document).on('click', 'a.comment-display-btn', function(e){
  e.preventDefault();
  $this = $(this);
  commentDisplayButtonClick($this);    
});

$(document).on('click', 'a.hide-comments', function(e){
  e.preventDefault();
  $this = $(this);
  var thishref = $this.attr('href');
  $('a.hide-comments[href="' + thishref + '"]').hide();
  $('a.comment-display-btn[href="' + thishref + '"]').removeClass("hidden");
  $('a.comment-display-btn[href="' + thishref + '"]').css("display","");
  $(thishref).addClass('hidden');
});


function showComments(blog_id, post_id, comments, startNum, endNum, author_username)
{
  var commentContent = '';
  var commentAuthor = '';
  var regex = new RegExp("\n", "g"); 

  var urlRegex =/(\b(https?):\/\/(www)?\.amazon[-A-Z0-9+&@#\/%?=~_|!:,.;]*[-A-Z0-9+&@#\/%=~_|])/ig;

  for (var i = startNum;i < endNum;i++) {

    try {

      comments[i]["text"] = comments[i]["content"].replace(urlRegex, function(url) {  
        var amazonUrl = new URL(url);
        if (amazonUrl["search"].length > 0)
          amazonUrl["search"] += "&tag=se-comment-20";
        else
          amazonUrl["search"] += "?tag=se-comment-20";
        return amazonUrl.href;
      });
    }
    catch (e) {}
    
    commentAuthor = comments[i]["author"];
    commentContent += '<a name="' + comments[i]["id"] + '"></a>';
    commentContent += '<div class="comment';

    var isAuthor = false;
    var isStaff = false;
    var staffLabel = "";
    // Try to look up staff slug by comment author
    var staffSlug = (function(){
      for (var i=0,j=_seStaff.length;i<j;i++) {
        if (_seStaff[i].name == commentAuthor) {
          return _seStaff[i].slug;
        }
      }
      return "";
    })();

    // TODO: Fix this (`commentAuthor` is name and `author_username` is username)
    if (commentAuthor == author_username) {
      isAuthor = true;
      // If author is not staff, build a URL with their username
      if (staffSlug === "") {
        staffSlug = "user/profile/" + commentAuthor.trim().replace(" ", "-").toLowerCase();
      }
      commentContent += ' author';
    } else if (staffSlug !== "") {
      commentContent += ' staff';
      isStaff = true;
      staffLabel = '<span class="label staff-user">Staff</span>';
    }
    
    commentContent += '" id="' + comments[i]["id"] + '">';
    
    commentContent += '<footer class="metadata">';
    if (isStaff || isAuthor)
      commentContent += '<a href="https://www.seriouseats.com/' + staffSlug + '" class="commenter';
    else
      commentContent += '<span class="commenter"';

    commentContent += '">' + comments[i]["author"];

    if (isStaff || isAuthor)      
      commentContent += '</a>';
    else
      commentContent += '</span>';

    commentContent += staffLabel + '<span>';
    var comment_date = comments[i]["date"];
    var comment_year = parseInt(comment_date.substr(0,4));
    var comment_month = parseInt(comment_date.substr(5,2));
    var comment_day = parseInt(comment_date.substr(8,2));
    var comment_hour = parseInt(comment_date.substr(11,2));
    // Pad with leading zero
    var comment_minute = ("0" + parseInt(comment_date.substr(14,2))).slice(-2);
    var comment_ampm = comment_hour >= 12 ? 'PM' : 'AM';
    comment_hour = comment_hour % 12;
    comment_hour = comment_hour ? comment_hour : 12;
    commentContent +=  comment_hour + ":" + comment_minute + " " + comment_ampm + " on " + comment_month + "/" + comment_day + "/" + comment_year + '</span></footer>';
    //commentContent += '<div class="comment-body"><div class="recipe-comment-checkbox"><input type="checkbox" name="include-comment" class="include-comment" value="' + comments[i]["comment_id"] + '"';
    commentContent += '<div class="comment-body">';
/*
    // check to see if this comment is in the recipeCommentsToInclude array
    if ((jQuery.inArray((comments[i]["comment_id"]), recipeCommentsToInclude)) >= 0) {
      commentContent += 'checked ';
    }
*/
    // commentContent += '</div>';
    if (comments[i]["rating"] && (comments[i]["rating"] > 0))
    {
      var star_full = '<i class="icon icon-nav-star-full"></i>';
      var star_empty = '<i class="icon icon-nav-star-empty"></i>';
      commentContent += '<div class="rating rating-user"><span class="label">recipe rating:</span><span class="info rating-stars">';
      for (var j = 0; j < 5; j++) {
        
        if (j < comments[i]["rating"])
          commentContent += star_full;
        else
          commentContent += star_empty;
      }
      commentContent += '</span></div>';
    }
    commentContent += '<div class="comment-content">' + comments[i]["text"].replace(regex, "<br />") + '</div></div>';
    commentContent += '</div>';   
  }
// #raw  

  $("section#comments-" + post_id).find("#commentSection").html(commentContent);

  // $('#commentSection').html(commentContent);

/* TODO -- move to printer page
  // Recipe post - checked comments for printing
  $('input[name$="include-comment"]').change(function() {
    
    var comment_id = $(this).attr('value');
    $("div.comment#" + comment_id).toggleClass("comment-print");

    if ($(this).is(':checked')) { 
      var arrayIndex = (jQuery.inArray(comment_id, recipeCommentsToInclude));
      if (arrayIndex < 0) {
        recipeCommentsToInclude.push(comment_id);
        comments_list = recipeCommentsToInclude.join();
      }
    }
    else {
      var arrayIndex = (jQuery.inArray(comment_id, recipeCommentsToInclude));
      if (arrayIndex >= 0) {
        recipeCommentsToInclude.splice(arrayIndex,1);
        comments_list = recipeCommentsToInclude.join();
      }
    }
  });
*/
}

function showCommentSection(post_id, comments)
{
  // if (!commentsDisplayed)
  // {
    var comment_container = $("section#comments-" + post_id);
    
    var author_username = comment_container.data("author-username");
    var blog_id = comment_container.data("blog-id");

    if (comments == undefined)
      return;

    // console.log(comment_container, comments);

    if (comments.length > 0)
    {
      comment_count = comments.length + " <span>Comment";
      if (comments.length > 1)
        comment_count += "s";
    } else {
      comment_count = "<span>Post a Comment";
    }
    comment_count += "</span>";

  // #if CommentsOpen == True
  //   commentCountText = commentCount;               
  // #end if

    if (comment_container.find('#commentSection').length) 
    {
      if (comment_container.find('#postCommentCount').length)
      {
        comment_container.find("#postCommentCount").html(commentCountText);
      }
      if (comment_container.find(".entryCommentCount").length > 0) 
      {
        comment_container.find(".entryCommentCount").each(function() {
          $(this).html(comment_count);
        });
      }

      if (comments.length <= numCommentsPerPage) {
        showComments(blog_id, post_id, comments, 0, comments.length, author_username);
      } else {
        jumpToTopOfComments = false;
        $(".pagination-" + post_id).pagination(comments.length, {
          items_per_page:numCommentsPerPage,
          num_edge_entries:1,
          num_display_entries:9,
          next_show_always:false,
          prev_show_always:false,
          link_to:"#comments-" + post_id,
          author_username:author_username,
          blog_id:blog_id,
          comments:comments,
          post_id:post_id,
          callback:handlePaginationClick
        });

        jumpToTopOfComments = true;
      }
      commentsDisplayed = true;

      var display_button = $("a#comments-display-btn-" + post_id);
      if (display_button && comments.length > 0)
      {
        display_button.text("Show " + comments.length + " Comments");
        var comment_bubbles = $("div.content-main[data-id=" + post_id + "] span.comment-number");
        for (var i = 0; i < comment_bubbles.length; ++i) 
        {
          $(comment_bubbles[i]).text(comments.length);
        }
      }
    } 

  /*} else {
    clearInterval(intervalID);
  }*/
} // showCommentSection
        </script>
