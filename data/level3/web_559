<script>
 function global_breakout(id, ad_unit)
{
  try {
    var iframe = document.getElementById(id);
    var regex = new RegExp(ad_unit, 'ig');
    var unit_name = id.match(regex)[0];
    var bcId = unit_name + "-breakout";
    var breakout_container = $('.' + bcId).filter(':visible')[0] || null;

    if (!iframe || !breakout_container)
      return;

    var iframe_body= iframe.contentDocument || iframe.contentWindow.document;
    var iframe_content= iframe_body.getElementsByTagName('body')[0].innerHTML;
    breakout_container.innerHTML = iframe_content;

    $ ("div.auto-ad[data-ad-unit=" + ad_unit + "]").hide();

  } catch(e) {
    // console.log(e); 
  }
}
</script>
