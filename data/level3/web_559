    <script type="text/javascript">
     star_enter = function() {
                  var $stars = $(this).siblings("i").andSelf();
                  var id = $(this).index();
                  $stars.slice(0, id + 1).removeClass("icon-nav-star-empty").addClass("icon-nav-star-full");
                }

                star_leave = function() {
                  var $stars = $(this).siblings("i").andSelf().not(".fixed");
                  var id = $(this).index();
                  $stars.slice(0, id + 1).removeClass("icon-nav-star-full").addClass("icon-nav-star-empty");
                }

                $("form#comments_form span.rating-stars i.icon").hover(star_enter, star_leave);
                $("form#comments_form span.rating-stars i.icon").click(function(event) {
                  var $stars = $(this).siblings("i").andSelf();
                  var id = $(this).index();
                  $stars.slice(0, id + 1).removeClass("icon-nav-star-empty").addClass("icon-nav-star-full fixed");
                  $stars.slice(id + 1, $stars.length).removeClass("icon-nav-star-full fixed").addClass("icon-nav-star-empty");
                  $("input[name=star_rating").val(id + 1);
                });
    </script>
    <p class="requirements">
     Comments can take a minute to appear—please be patient! Some comments may be held for manual review.
    </p>
    <div class="formCommentPreview">
     <input class="btn" id="comment-post-41164" name="post" type="submit" value="Post Comment">
     </input>
    </div>
   </form>
   <div id="previewingComment">
    <h5>
     PREVIEW YOUR COMMENT
    </h5>
    <div class="comment preview">
     <div class="comment-body">
      <p id="preview-41164">
