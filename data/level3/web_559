<script>
 const videoContainerEl = document.querySelector(".c-inline-video-container");
    let bcVideoIntersectionObserver;

    var inlinePlayer,
        inlinePlayerHTML,
        inlinePlayerData = {
          accountId: "4841592010001",
          playerId: "ryfRP95Xf",
          playlistId: "5998792392001"
        };

    inlinePlayerHTML = '<video-js id="bc-inline-player" data-playlist-id="' + inlinePlayerData.playlistId + '"  data-account="' + inlinePlayerData.accountId + '" data-player="' + inlinePlayerData.playerId + '" data-embed="default" class="video-js vjs-fluid" playsinline controls></video-js>';
    document.getElementById("se-bc-inline-video-container").innerHTML = inlinePlayerHTML;

    let seen_video = false;
    let video_floated = false;
    let user_closed_video = false;

    const hideBrightcove = () => {
      const player = document.getElementById("se-bc-inline-video-container");
      player && player.classList.remove('c-floating-video')
      bcPlayer = bc(document.getElementById('bc-inline-player'));
      bcPlayer.pause();
      user_closed_video = true;
      closeBCButton.hidden = true
    }

    var s = document.createElement("script");
    s.src = "https://players.brightcove.net/" + inlinePlayerData.accountId + "/" + inlinePlayerData.playerId + "_default/index.min.js";
    document.body.appendChild(s);


    const bcVideoEl = document.getElementById("se-bc-inline-video-container");
    const closeBCButton = document.createElement('div');
    closeBCButton.classList.add('c-button--close');
    closeBCButton.style.top = '-10px';
    closeBCButton.hidden = true
    closeBCButton.addEventListener('click', hideBrightcove);
    bcVideoEl.appendChild(closeBCButton)

    const handleBCVideoIntersection = (entry) => {
      if (user_closed_video) return
      if (entry.intersectionRatio == 1) {
        if (!seen_video) {
          bcPlayer = bc(document.getElementById('bc-inline-player'));
          bcPlayer.play();
        }
        seen_video = true;

        if (video_floated) {
          bcVideoEl.classList.remove("c-floating-video");
          closeBCButton.hidden = true
        }
      } else if (entry.intersectionRatio <= 0.1) {
        // don't float if the video hasn't been in view yet
        if (!seen_video) return;
        bcVideoEl.classList.add("c-floating-video");
        window._seAds.positionVideoEl();
        video_floated = true;
        closeBCButton.hidden = false
      }
    }

    const BCHandleOnload = () => {
      bcPlayer = bc("bc-inline-player");
      bcPlayer.muted(true);
      bcVideoIntersectionObserver = seGlobalMethods.observeIntersection(videoContainerEl, handleBCVideoIntersection)
      setBrightcoveAdTargeting();

      if(typeof seGlobalMethods === "object" && typeof seGlobalMethods.addListenerDocumentHidden === "function") {
        seGlobalMethods.addListenerDocumentHidden(bcVisibilityChange);
      }

      bcPlayer.on("loadedmetadata", function() {
        bcPlayer.muted(true);
      });

      bcPlayer.on('duringplaylistchange', () => {
        bcPlayer.playlist.shuffle();
        bcPlayer.playlist.repeat(true);
      });

      bcPlayer.on('loadstart', () => {
        const videoTitle = bcPlayer.mediainfo.name;
        const videoLink = bcPlayer.mediainfo.link && bcPlayer.mediainfo.link.url;
        const titleEl = document.getElementById('se-bc-video-title')
        titleEl.href = videoLink;
        titleEl.querySelector('h2').innerText = "Don't miss: " + videoTitle;
      });

    }
    s.onload = BCHandleOnload;

    function setBrightcoveAdTargeting() {
      if (typeof URL !== "function") return;

      bcPlayer = bc("bc-inline-player");

      const environment = (typeof seGlobalState === "undefined" || seGlobalState.staging === true) ? "dev" : "live";
      const postPath = window.location.pathname.substring(0, 40);
      const playerSize = bcPlayer.currentWidth() || -1;
      const playerMode = playerSize < 400 ? 'small' : 'medium';

      let postParam = "";
      const contentMain = document.getElementById("bc-inline-player").closest(".content-main");
      const postId = contentMain && contentMain.getAttribute("data-id");
      if (postId) {
        postParam = "&post_id=" + postId;
      }

      try {
        const ima3Url = new URL(bcPlayer.ima3.settings.serverUrl);
        ima3Url.searchParams.set("cust_params",
          "environment=" + environment +
          "&player=" + playerMode +
          "&size=" + playerSize +
          postParam +
          "&path=" + postPath
        );
        bcPlayer.ima3.settings.serverUrl = ima3Url.href;
        if (window.innerWidth <= 972)
        {
          mobile_href = bcPlayer.ima3.settings.serverUrl.replace("SE_Sidebar_Video_640x480", "SE_Video_Mobile_300x400");
          bcPlayer.ima3.settings.serverUrl = mobile_href;
        }        
      } catch(e) { console.error(e);}

    }

    function bcVisibilityChange(event) {
      bcPlayer = bc("bc-inline-player");
      if (seGlobalState["documentHidden"]) {
        bcPlayer.pause();
      } else {
        bcPlayer.play();
      }
    }
</script>
