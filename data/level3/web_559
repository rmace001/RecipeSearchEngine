ayer.ima3.adPlayer.muted(!0))},setMetadata:function(){var e=this,t=e._player.mediainfo.link&&e._player.mediainfo.link.url,i=e._player.mediainfo.name;i&&""!=i.trim()&&(e.currentVid.url=t?(e.linkEle.attr("href",t),e.linkEle.attr("data-click-id",t),t):(e.linkEle.removeAttr("href"),e.linkEle.attr("data-click-id",i.trim()),""),e.titleEle.text(i.trim()),e.currentVid.title=i.trim()),-1<(e._player.mediainfo.tags||[]).indexOf("sponsored")?e.currentVid.sponsored=!0:e.currentVid.sponsored=!1,e.linkEle.attr("data-sponsored",e.currentVid.sponsored)},setAdTargeting:function(){if("function"==typeof URL){var e=this._player,t="undefined"==typeof seGlobalState||!0===seGlobalState.staging?"dev":"live",i=window.location.pathname.substring(0,40),a=$(".sidebar").width()||-1,r=a<400?"small":"medium",n="";void 0!==this.postId&&0<this.postId.length&&(n="&post_id="+this.postId);try{var o=new URL(e.ima3.settings.serverUrl);o.searchParams.set("cust_params","environment="+t+"&player="+r+"&size="+a+n+"&path="+i),e.ima3.settings.serverUrl=o.href}catch(e){}}},setPlayerListeners:function(){var a=this;a._player.playlist.repeat(!0),a._player.on("ads-load",a.muteAdPlayer.bind(a)),a._player.on("ads-ad-started",a.muteAdPlayer.bind(a)),a._player.on("ima3-ads-manager-loaded",a.muteAdPlayer.bind(a)),a._player.on("ima3-loaded",a.muteAdPlayer.bind(a)),a._player.on("ima3-started",a.muteAdPlayer.bind(a)),a._player.on(["ads-ad-ended","ima3-complete","ads-ad-skipped","adserror","ads-allpods-completed"],function(e){a.videoInViewCheck()}),a._player.on("loadstart",function(e){a.cancelPauseReport("loadstart"),a.setMetadata(),a.currentVid.index=a._player.playlist.currentItem(),a.setCookie(a.cookieName,a.currentVid.index)}),a._player.muted(!0),a.muted=!0,a._player.on("click",function(e){a.userInteraction=!0}),a._player.on("mousedown",function(e){a.userInteraction=!0}),a._player.on("touchstart",function(e){a.userInteraction=!0}),a._player.on("volumechange",function(e){if(!0===a.userInteraction&&(a._player.muted()&&!a.muted||!a._player.muted()&&a.muted)){a.muted=!!a._player.muted(),a.muted||$("video").each(function(){try{$(this).is(a.$videoEle)||this.pause()}catch(e){}});var t=a.muted?"mute":"unmute",i=a.currentVid.title;a.sendGAEvent(t,i,!1)}}),a._player.on("fullscreenchange",function(e){if(!0===a.userInteraction){a.fullScreen=!!a._player.isFullscreen();var t=a.fullScreen?"fullscreen":"restore",i=a.currentVid.title;a.sendGAEvent(t,i,!1)}}),a._player.on("play",function(e){if((a.resumePlayOnScroll=!0)===a.userInteraction&&(!0!==a.scrollThreshold&&(a.scrollThreshold=!0,a.preventPlayReport=!1,a.muted&&(a._player.muted(!1),a.muted=!1)),!a.preventPlayReport)){var t=a.currentVid.title;a.sendGAEvent("play",t,!1)}}),a._player.on("pause",function(e){if(!0===a.userInteraction&&(a.inView||a.fullScreen)){a.preventPlayReport=!1,a.resumePlayOnScroll=!1;var t=a.currentVid.title;a.pauseTimeout=setTimeout(function(){a.sendGAEvent("pause",t,!1)},a.pauseCheckTime)}}),a._player.on("seeking",function(e){a.cancelPauseReport("seeking"),a.preventPlayReport=!0}),a._player.on("ended",function(e){a.cancelPauseReport("ended"),a.preventPlayReport=!0}),a._player.on("abort",function(e){a.cancelPauseReport("abort"),a.preventPlayReport=!0}),a._player.on("emptied",function(e){a.cancelPauseReport("emptied"),a.preventPlayReport=!0})},videoInViewCheck:function(){var e=this;if(!0===e.scrollThreshold){if(!0===e.autoplayInView){var t=seGlobalState.documentHidden||!1,i=seGlobalMethods.inViewportVert(e.$outerWrap,e.$outerWrap.outerHeight()),a=e._player.paused(),r=!!e._player.ads&&e._player.ads.inAdBreak(),n=!!e._player.ima3&&!!e._player.ima3.adPlayer;e.inView=!t&&i,r&&e.inView?(e._player.pause(),n&&e._player.ima3.adPlayer.play()):r&&!e.inView?(n&&e._player.ima3.adPlayer.pause(),e._player.pause()):e.inView&&a&&e.resumePlayOnScroll?(e.preventPlayReport=!0,e._player.play()):e.inView||e.fullScreen||e._player.pause()}}else window.pageYOffset>e.scrollThreshold&&(e.scrollThreshold=!0)},initInViewCheck:function(e){var t=this;seGlobalState&&seGlobalState.sidebarVideo&&(t.inViewCheckInt=setInterval(function(){this.canCheckVid&&(this.canCheckVid=!1,this.videoInViewCheck())}.bind(t),e),seGlobalState.$window.on("load scroll",function(){t.canCheckVid=!0}),"object"==typeof seGlobalMethods&&"function"==typeof seGlobalMethods.addListenerDocumentHidden&&seGlobalMethods.addListenerDocumentHidden(t.videoInViewCheck.bind(t)))}},sidebarController.Sidebar=function(e){if(this.$ele=e,this.$bngEle=this.$ele.find(".sidebar-bng"),this.$svpEle=this.$ele.find(".video-sidebar-player-wrap"),this.$entryContainer=this.$ele.siblings(".entry-container"),971<window.innerWidth){this.$bngEle.length&&(this.bng=new sidebarController.BNG(this.$bngEle,this)),this.svpData=seGlobalState&&seGlobalState.sidebarVideoData;try{void 0!==this.svpData&&(this.svpData.postID=this.$entryContainer.parent().attr("data-id"))}catch(e){void 0!==this.svpData&&(this.svpData.postID="")}this.$svpEle.length&&this.svpData&&this.svpData.videoIDs&&this.svpData.videoIDs.length&&(seGlobalState.sidebarVideo=!0,this.svp=new sidebarController.SidebarVid(this.$svpEle,this.svpData))}else this.$ele.empty()},sidebarController.Sidebar.prototype={scrollHandler:function(e){var t=this.bng,i=this.$entryContainer.offset().top+this.$entryContainer.height();t.scrollHandler(e,i)}},sidebarController.scrollCheck=function(){var i=window.pageYOffset+sidebarController.topOffset;$.each(sidebarController.sidebars,function(e,t){t.scrollHandler(i)})},971<window.innerWidth&&$(window).on("load scroll",function(){sidebarController.scrollCheck()}),sidebarController.sidebars.push(new sidebarController.Sidebar($("section.sidebar").last()))})}

          sidebarWrapper.apply(null, [true]);

    })();
</script>
<script>
 $(function(){

  function FiniteScroll(linkedEntries, settings) {
    this.defaultSettings = {
      postSelector: ".content-main",
      historyThreshold: 25, // percent above and below the window midpoint; controls when page title and history are updated 
      loadThreshold: 1000 // pixels beyond window bottom; controls when a new post is loaded in
    };
    this.settings = this.setSettings(settings, this.defaultSettings);

    this.postSelector = this.settings.postSelector;
    this.historyThreshold = Math.abs(this.settings.historyThreshold / 100);
    this.loadThreshold = this.settings.loadThreshold;

    // setHistoryThresholdLines sets:
    //   this.roofLine
    //   this.floorLine
    this.setHistoryThresholdLines();

    this.lastEntry = this.getLastEntry();
    this.lastEntryID = this.lastEntry.data("id") || 0;
    this.currentEntryID = this.lastEntryID;
    this.nextUrl = linkedEntries;
    this.counter = 0;
    // this.checkTime = 300;
    this.checkTime = 500;
    this.historyTry = false;
    this.loadTry = false;

    this.init();
  } // FiniteScroll constructor

  FiniteScroll.prototype = {

    init: function() {
      this.setListeners();
    },

    // Determines finite scroll settings based on defaults and user overrides
    setSettings: function(userSettings, defaultSettings) {
      if(!userSettings) { return defaultSettings; }
      var settings = defaultSettings;
      Object.keys(userSettings).forEach(function(key, index) {
        settings[key] = userSettings[key];
      });
      return settings;
    },

    // Determines 'roof' and 'floor' lines to update history and title
    setHistoryThresholdLines: function() {
      var windowHeight = window.innerHeight,
          thresholdPx = this.historyThreshold * windowHeight,
          midLine = windowHeight / 2;
      this.roofLine = Math.round(midLine - thresholdPx);
      this.floorLine = Math.round(midLine + thresholdPx);
    },

    getAllEntries: function() {
      return $( this.postSelector );
    },

    getLastEntry: function() {
      return this.getAllEntries().last();
    },

    /**
     * Set/unset the nopin attribute on pinnable images so Pinterest pinning tool
     * will show only the current post's images
     */
    updateNoPin: function() {
      var obj = this;
      $('img[data-pin-description]').attr("nopin", "nopin");
      $('.content-main[data-id="' + obj.currentEntryID + '"] img[data-pin-description]').removeAttr("nopin");
    },

    /**
     * Update meta tags
     */
    // updateOGMeta: function(title) {
    //   //var ldjson = JSON.parse($("script[type='application/ld+json']").text());
    //   //this.log("updateOGMeta:  ld+json:" + $("script[type='application/ld+json']").text());

    //   var obj = this;
    //   // obj.currentEntryID

    //   var metaImageURL =  $(recipeElem).find('.featured-image img').attr('src');

    //   $('meta[property="og:image"]').attr('content', metaImageURL);
    //   this.log("Updated: og:image url: " + $('meta[property="og:image"]').attr("content"));

    //   var metaDescription =   $(recipeElem).find('.featured-image img').attr('data-pin-description');
    //   $('meta[property="og:description"]').attr('content', metaDescription);
    //   $('meta[name="description"]').attr('content', metaDescription);

    //   var metaUrl =  $(recipeElem).find('link[rel="canonical"]').attr("href");
    //   $('meta[property="og:url"]').attr('content', metaUrl);
    //   $('meta[property="og:title"]').attr('content', title);
    // },

    updateCurrentEntry: function(entryID, title, url) {
      var obj = this;
      obj.currentEntryID = entryID;
      var seTitle = title.indexOf(" | Serious Eats") < 0 ? title + " | Serious Eats" : title;

      try {
        // TODO: Can add a data object in here w/ a popstate listener elsewhere if we want to better control browser navigation
        history.replaceState(null, seTitle, url);
      } catch (err) {}
      document.title = seTitle;

      obj.updateNoPin();
      // obj.updateOGMeta(seTitle);

    }, // updateCurrentEntry

    hasEntryChanged: function() {

      var obj = this,
          allEntries = obj.getAllEntries(),
          winTop = window.pageYOffset,
          winBottom = winTop + window.innerHeight,
          currentRoofLine = obj.roofLine + winTop,
          currentFloorLine = obj.floorLine + winTop;

      allEntries.each(function(i) {
        // TODO: Try to cache some of these
        var thisEntry = $(this),
            betweenEntriesAd = thisEntry.next(".ad-wrapper").find(".auto-ad").filter(':visible').first(), // between-posts ad
            betweenEntriesAdLength = betweenEntriesAd.length, // 0 or 1
            betweenEntriesAdHeight = betweenEntriesAdLength && betweenEntriesAd.outerHeight(true), // 0 or height
            thisEntryID = thisEntry.data("id") || null,
            thisEntryTop = thisEntry.offset().top,
            // post top + post height (including margins if no ad) - margin-top (if no ad) + ad height (including margins)
            thisEntryBottom = Math.round(thisEntryTop + thisEntry.outerHeight(!betweenEntriesAdLength) - (parseInt(thisEntry.css("margin-top")) * (1-betweenEntriesAdLength)) + betweenEntriesAdHeight);

        if(thisEntryID === null || thisEntryTop > winBottom || thisEntryBottom < winTop) {
          // Entry ID is null (should never happen) or entry is off the page; continue loop
          return true;
        }

        // Here, some part of the post is in view
        if(thisEntryBottom > currentFloorLine) {

          // This is current article
          if(thisEntryID != obj.currentEntryID) {
            // Update current post/history
            obj.updateCurrentEntry(thisEntryID, thisEntry.data("title"), thisEntry.data("permalink"));
          }

        } else if(thisEntryBottom < currentRoofLine && i < allEntries.length - 1) {

          // The next entry is the current article; no need to repeat the above logic
          // TODO: The between-entries ad gives a little shift to this... may want to check the top of the next entry against roofLine as well.
          var nextEntry = $(allEntries[i+1]),
              nextEntryID = nextEntry.data("id") || null;

          if(nextEntryID !== null && nextEntryID != obj.currentEntryID) {
            // Update current post/history
            obj.updateCurrentEntry(nextEntryID, nextEntry.data("title"), nextEntry.data("permalink"));
          }

        }

        // Else, the mid-posts break is in the middle of the page, and we don't update the history
        // break loop
        return false;

      });
      
    }, // hasEntryChanged

    loadNextEntry: function() {
      var obj = this,
          winBottom = window.pageYOffset + window.innerHeight,
          lastEntryBottom = obj.lastEntry.offset().top + obj.lastEntry.outerHeight(true);

      if(lastEntryBottom - obj.loadThreshold > winBottom) {

        obj.loadTry = false;
        setTimeout(function(){ obj.fsTOInit(); }, obj.checkTime);

      } else {

        var url = obj.nextUrl.pop();

        if(!url) {return;}

        obj.counter++;

        var requestUrl = "/json/entry?record=" + url + "&counter=" + obj.counter;
        var request = $.ajax({
          url: requestUrl
        }).done(function(data) {

          try {

            try {
              if (!data.hasOwnProperty("body")) {
                return;
              }
              obj.lastEntry.after(data["body"]);
            } catch (err) {}

            try {
              _seAds.targeting.extractTargetingFromPost(data);
            } catch (err) {}

            // The new ad container elements should already be loaded in here.
            typeof _seAds.render === "function" && _seAds.render();

            // TODO: Do we need this?
            // obj.hasEntryChanged();

            // try {
            //   var $lastRecipeEntry = $('.hrecipe').last();
            //   $lastRecipeEntry.find('.recipe-introduction-body p.caption').insertAfter($lastRecipeEntry.find(".recipe-main-photo"));
            // } catch (err) {}

            try {
              self.COMSCORE && COMSCORE.beacon({c1: "2", c2: "14627434"});
            } catch (err) {}

            try {
              dataLayer.push({"event" : "gaPageview"});
              dataLayer.push({
                "event": "gaEvent",
                "category": "finite-scroll",
                "action": "load-next-entry",
                "label": obj.counter,
                "nonInteraction": false
              });
            } catch (err) {}            

            try {
              var $relish_button = $("span#relish-button[data-recipe-id='" + data.post_id + "']");
              if ($relish_button.length > 0)
              {
                if (window.fexyRelish === undefined)
                {
                  $.ajax({
                    url: "https://relish.com/scripts/relish.js?key=fad43e07-2ce3-4c04-97a2-ffd099ecbfd9",
                    dataType: "script",
                    cache: true,
                    success: function() {
                      window.fexyRelish.addShopButton($relish_button, null, data.post_id);
                    },
                    async: true
                  });
                }
                else
                {
                  // fexyRelish.addShopButton(BUTTON_SELECTOR, INGREDIENTS_SELECTOR, RECIPE_ID);
                  window.fexyRelish.addShopButton($relish_button, null, data.post_id);
                }
              }
            }
            catch (err) {}

            // Set up new Pinterest buttons
            window.parsePinBtns && window.parsePinBtns();
            seGlobalState.pinImagesLoaded = false;

          } catch (err) {}

          obj.lastEntry = obj.getLastEntry();

          if(obj.nextUrl.length) {
            obj.loadTry = false;
            setTimeout(function(){ obj.fsTOInit(); }, obj.checkTime);
          }
        }); // ajax done
      }
    }, // loadNextEntry

    fsTOInit: function() {
      var obj = this;
      if(obj.loadTry) {
        obj.loadNextEntry();
      } else {
        setTimeout(function(){ obj.fsTOInit(); }, obj.checkTime);
      }
    },

    setListeners: function() {
      var obj = this;
      $(window).on('load resize', function(){
        obj.setHistoryThresholdLines();
      });
      $(window).on('scroll', function() {
        obj.loadTry = true;
        
        if(obj.historyTry) {
          return;
        }

        obj.historyTry = true;
        obj.hasEntryChanged();

        setTimeout(function() {
          obj.historyTry = false;
        }, obj.checkTime - 100);

      });
      obj.fsTOInit();
    } // setListeners

  }; // FiniteScroll prototype

  var linkedEntries = [];
    linkedEntries.push("https://www.seriouseats.com/2012/06/french-in-a-flash-boursin-and-gruyere-spinach-and-artichoke-gratin-dip.html");
  if(linkedEntries.length < 1) {
    return;
  }

  var pageFS = new FiniteScroll(linkedEntries);

}); // END document.ready
</script>

