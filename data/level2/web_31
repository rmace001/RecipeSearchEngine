    <div class="auto-ad" data-ad-mapping="horizontal_flex_mapping" data-ad-size="[[300,250]]" data-ad-unit="SE_Rectangle_1" data-refresh-time="30000" id="category-top-billboard">
    </div>
   </div>
   <div class="bottom-sticky-ad">
    <div class="ad-wrapper touch-display-none">
     <div class="auto-ad" data-ad-mapping="horizontal_short_mapping" data-ad-size="[[970,250], [970,90], [728,90]]" data-ad-unit="SE_Horizontal_1" data-refresh-time="30000" id="category-top-leader-sticky">
     </div>
