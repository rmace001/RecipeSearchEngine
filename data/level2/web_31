  <script>
   try {
  setTimeout(function() {
    dataLayer.push({
      "event" : "gaEvent",
      "category" : "dwell",
      "action" : "1 minute elapsed",
      "label" : "",
      "nonInteraction": false
    })
  }, 60000);  
