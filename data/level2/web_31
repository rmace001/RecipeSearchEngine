        </a>
       </li>
      </ul>
     </nav>
    </section>
    <section class="c-category-flex__header-right">
     <div class="c-sort-toggle">
      <span class="c-sort-toggle__label">
       SORT BY:
      </span>
      <span class="c-sort-toggle__toggle" onclick="openSortByToggleDropdown(event)">
       MOST POPULAR
       <div class="c-sort-toggle__dropdown">
        <span class="c-sort-toggle__option" data-sort-type="popular" onclick="activateSortType(event)">
         MOST POPULAR
        </span>
        <span class="c-sort-toggle__option c-sort-toggle__option--inactive" data-sort-type="latest" onclick="activateSortType(event)">
         MOST RECENT
        </span>
       </div>
      </span>
     </div>
     <script>
      function closeSortByToggleDropdown(e) {
      var dropdownEls = document
        .querySelectorAll('.c-sort-toggle__dropdown');
      dropdownEls && dropdownEls.forEach(function(el) {
        el.classList.remove('c-sort-toggle__dropdown--active')
      })
      window.removeEventListener('click', closeSortByToggleDropdown);
    }

    function openSortByToggleDropdown(e) {
console.log('open')
      var dropdownEl = e.target.parentElement.
        querySelector('.c-sort-toggle__dropdown');
      dropdownEl && dropdownEl.classList
            .add('c-sort-toggle__dropdown--active')
      setTimeout(function() {
        window.addEventListener('click', closeSortByToggleDropdown);
      }, 10)
    }

    function activateSortType(e) {
      e.stopPropagation();
      var sort = e.target.getAttribute('data-sort-type')
      var isLatest = window.location.search.includes('latest')

      if (isLatest && sort == 'popular') {
        window.location.search = 'sort=popular'
      } else if (!isLatest && sort == 'latest') {
        window.location.search = 'sort=latest'
      }
      closeSortByToggleDropdown();
    }
     </script>
